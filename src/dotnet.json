{
  "id": "dotnet",
  "version": "0.1.3",
  "description": "C# support for gauge + .NET Core 2.0",
  "run": {
    "windows": [
      "launcher.cmd",
      "--start"
    ],
    "linux": [
      "launcher.sh",
      "--start"
    ],
    "darwin": [
      "launcher.sh",
      "--start"
    ]
  },
  "init": {
    "windows": [
      "launcher.cmd",
      "--init"
    ],
    "linux": [
      "launcher.sh",
      "--init"
    ],
    "darwin": [
      "launcher.sh",
      "--init"
    ]
  },
  "gaugeVersionSupport": {
    "minimum": "1.0.6",
    "maximum": ""
  },
  "lspLangId": "csharp",
  "gRPCSupport": true
}